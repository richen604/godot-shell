[application]

config/name="Godot 2D Project"
config/description="A performance-optimized 2D project template for rapid prototyping and game jams"
config/version="1.0.0"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[audio]

driver/driver="PulseAudio"
driver/mix_rate=44100
driver/output_latency=15

[debug]

settings/stdout/print_fps=false
settings/stdout/verbose_stdout=false
settings/crash_handler/message="Please submit a bug report with steps to reproduce the issue."

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/resizable=true
window/stretch/mode="canvas_items"
window/stretch/aspect="keep"
window/vsync/vsync_mode=1

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

