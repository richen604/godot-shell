[layer_names]

2d_physics/layer_1="Player"
2d_physics/layer_2="World"
2d_physics/layer_3="Enemies"
2d_physics/layer_4="Items"
2d_physics/layer_5="Triggers"

[physics]

common/max_physics_steps_per_frame=8
common/physics_jitter_fix=0.5
2d/default_gravity=980
2d/default_linear_damp=0.1
2d/default_angular_damp=1.0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/canvas_textures/default_texture_filter=1
2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
