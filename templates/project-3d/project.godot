; Engine configuration file.
; It's best edited using the editor UI and not directly,
; but the file can also be edited by hand.
; Learn more at https://docs.godotengine.org/en/stable/getting_started/step_by_step/project_setup.html

[application]

config/name="Godot 3D Project"
config/description="A performance-optimized 3D project template for rapid prototyping and game jams"
config/version="1.0.0"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[audio]

driver/mix_rate=44100
driver/output_latency=15
buses/default_bus_layout="res://default_bus_layout.tres"
driver/enable_input=true
general/2d_panning_strength=0.5
general/3d_panning_strength=1.0

[debug]

settings/stdout/print_fps=false
settings/stdout/verbose_stdout=false
settings/crash_handler/message="Please submit a bug report with steps to reproduce the issue."

[debug]
gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
file_logging/enable_file_logging=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/size/resizable=true
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/vsync/vsync_mode=1
window/size/always_on_top=false
window/size/transparent=false
window/size/borderless=false
window/energy_saving/keep_screen_on=true
window/dpi/allow_hidpi=true

[filesystem]

import/blender/enabled=true
import/fbx/enabled=true
import/fbx2gltf/enabled=true

[input_devices]
pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=true

