[layer_names]

3d_physics/layer_1="Player"
3d_physics/layer_2="World"
3d_physics/layer_3="Enemies"
3d_physics/layer_4="Items"
3d_physics/layer_5="Triggers"

[physics]

common/max_physics_steps_per_frame=8
common/physics_jitter_fix=0.5
common/physics_ticks_per_second=60
common/enable_object_picking=true
3d/default_gravity=9.8
3d/default_linear_damp=0.1
3d/default_angular_damp=0.1
3d/physics_engine="DEFAULT"
3d/solver/contact_max_allowed_penetration=0.01

[rendering]

renderer/rendering_method="forward_plus"
renderer/rendering_method.mobile="mobile"
textures/canvas_textures/default_texture_filter=2
anti_aliasing/quality/msaa_3d=3
anti_aliasing/quality/screen_space_aa=1
anti_aliasing/quality/use_taa=false
anti_aliasing/quality/use_debanding=true
occlusion_culling/use_occlusion_culling=true
occlusion_culling/bvh_build_quality=2
mesh_lod/lod_change/threshold_pixels=2.0
global_illumination/gi/use_half_resolution=true
reflections/sky_reflections/texture_array_reflections=true
reflections/sky_reflections/ggx_samples=32
reflections/sky_reflections/roughness_layers=8
lights_and_shadows/directional_shadow/size=4096
lights_and_shadows/directional_shadow/soft_shadow_filter_quality=3
lights_and_shadows/positional_shadow/soft_shadow_filter_quality=2
lights_and_shadows/positional_shadow/atlas_size=4096
environment/ssao/enabled=true
environment/ssil/enabled=false
environment/glow/enabled=false
driver/threads/thread_model=2
limits/time/time_rollover_secs=3600
2d/snap/snap_2d_transforms_to_pixel=false
2d/snap/snap_2d_vertices_to_pixel=false
textures/default_filters/use_nearest_mipmap_filter=false
textures/default_filters/anisotropic_filtering_level=4
textures/vram_compression/import_etc2_astc=true
textures/lossless_compression/force_png=false

[compression]
formats/zstd/compression_level=3
formats/zlib/compression_level=6

[memory]
limits/message_queue/max_size_mb=32
